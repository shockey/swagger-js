{
  "requireCleanWorkingDir": false,
  "beforeChangelogCommand": "./build/check-for-breaking-changes.sh ${latestVersion} ${version}",
  "buildCommand": "npm run build",
  "increment": "conventional:angular",
  "src": {
    "commitMessage": "v%s",
    "tagName": "v%s",
    "push": false
  },
  "npm": {
    "tag": null
  },
  "github": {
    "release": true,
    "releaseName": "Swagger Client v%s Released!",
    "draft": true
  }
}